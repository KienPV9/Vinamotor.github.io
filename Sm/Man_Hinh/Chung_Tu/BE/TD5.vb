                LabMa_kho.Visible = ChkIs_Dc.Checked
                LabMa_khoN.Visible = ChkIs_Dc.Checked

                TxtMa_Kho.Visible = ChkIs_Dc.Checked
                TxtMa_KhoN.Visible = ChkIs_Dc.Checked

