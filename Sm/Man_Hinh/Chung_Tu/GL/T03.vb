        Dim iRow As Integer = -1
        iRow = DetailGRV.GetFocusedDataSourceRowIndex
        If iRow < 0 Then Exit Sub
        If Dv_DetailTmp.Item(iRow).Item("Tk_No").ToString.Trim = "" Then
